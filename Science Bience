/**/

#include <iostream>
#include <math.h>

int main(){
    int students, tot=0;
    double avr=0;
    std::cin>>students;
    double costs[students-1];
    for(int x=0;x<students;x++){
        std::cin>>costs[x];
        avr+=costs[x];
    }
    avr=floor((avr/(double)students)*100+0.5)/100;
    std::cout<<avr<<"\n";
    return 0;
}
