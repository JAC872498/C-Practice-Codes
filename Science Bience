/**/

#include <iostream>

int main(){
    int students, biggest=0, smallest=0;
    std::cin>>students;
    int costs[students-1];
    for(int x=0;x<students;x++){
        std::cin>>costs[x];
        if(costs[biggest]<=costs[x]){biggest=x;}
        if(costs[smallest]>=costs[x]){smallest=x;}
    }
    //std::cout<<costs[biggest]<<"\n"<<costs[smallest];
    while(costs[biggest]!=costs[smallest]){
        costs[biggest]-=0;
        costs[smallest]+=1;
    }
    std::cout<<costs[biggest]<<"\n"<<costs[smallest];
    return 0;
}
