	std::string l2[13]={"q","w","e","r","t","y","u","i","o","p","[","]","|"};
	std::string l3[11]={"a","s","d","f","g","h","j","k","l",";","'"};
	std::string l4[10]={"z","x","c","v","b","n","m",",",".","/"};
	std::string wrd,newWrd="";
	getline(std::cin,wrd);
	std::cout<<wrd<<"\n";
	for(int n=0;n<wrd.length();n++){
		std::cout<<"\n"<<wrd.substr(n,n+1);
	}
